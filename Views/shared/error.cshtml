    <div class="container">
        <header id="page-header"></header>
        <nav id="page-navigation" role="navigation">
            <div class="navbar navbar-default navbar-fixed-top">
                <div class="container">
                    @Html.ActionLink("BuildFeed", "index", new { controller = "front", area = "" }, new { @class = "navbar-brand" })
